<?xml version="1.0" ?>
<config>
	<params>
		<local-type>router</local-type>
                <destination-advert-enable>0</destination-advert-enable>
		<discovery-enable>1</discovery-enable>
		<discovery-port>4854</discovery-port>
		<discovery-iface>eth1</discovery-iface>
		<discovery-mcast-address>224.0.0.117</discovery-mcast-address>
		<discovery-interval>5</discovery-interval>
		<heartbeat-interval>5</heartbeat-interval>
		<heartbeat-threshold>3</heartbeat-threshold>
		<session-port>4854</session-port>
		<peer-type>emulated-router1</peer-type>
                <log-file>dlep-router1.log</log-file>
		<log-level>1</log-level>
	</params>
</config>
